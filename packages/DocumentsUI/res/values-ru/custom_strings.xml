<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Action bar title when the app is in standalone (file manager) mode [CHAR LIMIT=32] -->
    <string name="title_standalone">Файлы</string>

    <!-- Toast shown after successfully deleting files -->
    <string name="toast_success_delete">Файлы удалены</string>

    <!-- Title of the Delete confirmation dialog -->
    <string name="dialog_delete_confirm_title">Удалить файлы?</string>

    <!-- Menu item title that copies the selected documents [CHAR LIMIT=24] -->
    <string name="menu_copy">Копировать</string>
    <!-- Menu item title that cuts the selected documents [CHAR LIMIT=24] -->
    <string name="menu_cut">Вырезать</string>
    <!-- Menu item title that pastes the selected documents [CHAR LIMIT=24] -->
    <string name="menu_paste">Вставить</string>


    <!-- Text shown in a toast when the user copies files -->
    <plurals name="files_copied">
        <item quantity="one">Файл скопирован в буфер обмена</item>
        <item quantity="other">%d файлов были скопированы в буфер обмена</item>
    </plurals>

    <!-- Text shown in a toast when the user pastes files -->
    <plurals name="files_pasted">
        <item quantity="one">Файл был вставлен. Загрузку можно закончить в фоновом режиме.</item>
        <item quantity="other">%d Файлов были вставлены. Загрузку можно закончить в фоновом режиме.</item>
    </plurals>

    <!-- Text show in the progress dialog while files are being copied -->
    <string name="copy_in_progress">Копирование...</string>

    <!-- Text show in the progress dialog while files are being deleted -->
    <string name="delete_in_progress">Удаление...</string>

    <plurals name="dialog_delete_confirm_message">
        <item quantity="one">Этот файл будет удален!</item>
        <item quantity="other">Это окончательно удалит %d выбранных файлов!</item>
    </plurals>

    <string name="ok">OK</string>
    <string name="cancel">Отмена</string>
</resources>